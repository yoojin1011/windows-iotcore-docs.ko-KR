{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "layout": "Conceptual",
      "feedback_system": "None",
      "breadcrumb_path": "/windows/iot-core/breadcrumb/toc.json",
      "uhfHeaderId": "MSDocsHeader-Windows-IoT",
      "searchScope": [
        "windows-iot"
      ],
      "ms.prod": "windows-iot",
      "ms.technology": "IoT",
      "titleSuffix": "Windows IoT"
    },
    "fileMetadata": {
       "open_to_public_contributors": {
         "./*.md": true,
         "tutorials/*.md": true,
         "tutorials/quickstarter/**.md": true,
         "release-notes/*.md": true,
         "release-notes/commercial/**.md": true,
         "release-notes/insider/**.md": true
        }
    },
    "template": [],
    "dest": "windows-iotcore",
    "markdownEngineName": "markdig"
  }
}
